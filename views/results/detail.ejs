<%- include('../_layout/header'); %>

<h3 class="text-center">Result # <%= result.rollno %></h3>

<div class="row">
  <div class="col-sm-12 col-md-4 offset-md-4">
    <table class="table table-hover table-borderless">
      <tbody>
        <tr>
          <td>Roll No.</td>
          <td>:</td>
          <td><%= result.rollno %></td>
        </tr>
        <tr>
          <td>Name</td>
          <td>:</td>
          <td><%= result.name %></td>
        </tr>
        <tr>
          <td>Score</td>
          <td>:</td>
          <td><%= result.score %></td>
        </tr>
        <tr>
          <td>Date of Birth</td>
          <td>:</td>
          <td><%= result.dob.toDateString() %></td>
        </tr>
      </tbody>
    </table>
  </div>
</div>

<%- include('../_layout/footer'); %>